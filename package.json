{
  "name": "FlickrSearch",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build-css": "node-sass ./src/App.scss --recursive --output src",
    "watch-css": "node-sass --watch ./src/App.scss --recursive --output src",
    "build-js": "react-scripts build",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
